.messages .tags {
    font-weight: bold;
      float: left;
      line-height: 15px;
      padding-right: 5px;
}
.filter-block-append {
    overflow: hidden;
}
.filter-block-append .default {
    z-index: 5;
}
.filter-menu {
    background-color: #FFF;
    z-index: 10;
    width: 100%;
