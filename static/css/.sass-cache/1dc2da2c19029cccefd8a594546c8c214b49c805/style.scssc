		&:hover { background-position: 0 -22px; }
	}
	
	.filter-bottom, .filter-top {
		display: block;
		height: 20px;
		clear: both;
		background: url("../img/bg-filter_mood_bottom.png") no-repeat center bottom;
	}
	.filter-top { background: url("../img/bg-filter_mood_top.png") no-repeat center top; }



/**************/
/* Categories */
/**************/
	.categories-list {
		float: right;
		width: 68%;
	}
	.list-block {
	  border-top: 1px solid #dadada;
	  border-bottom: 1px solid #dadada;
	  border-right: 1px solid #dadada;
	  border-left: 3px solid #D04187;
	  list-style: none;
	  float: left;
	  text-align: left;
	  margin: 0 20px 5px 0;
	  a{
	    color: #000;
	    padding: 3px 5px;
	    display: block;
	    text-decoration: none;
	    &:hover{ background: #eaeaea; }
	  }
	  a.active { background: #eaeaea; font-weight: bold;}
	}


    
/****************/
/* Notification */
/****************/
	.ic-status{
		float: left;
		width: 12px;
		height: 12px;
		margin: 3px 5px 0 0;
		text-indent: -999em;
		background: url('../img/ic-notify.png') no-repeat ;
	}
	.ic-download-notify{ background-position: 0 -12px;}
	
	.avatar{ width: 35px;}
	.ic-notify{
		width: 12px;
		padding: 0 !important;
	}
	.middle{ vertical-align: middle !important;}
	.middle .grey{ font-size: 11px; }
	
	.table-list th, .table-list td{ border: none; }
	.table-list .centered{ text-align: center;}
	.table-striped tbody tr:nth-child(odd) td,
	.table-striped tbody tr:nth-child(odd) th{ background: #f6f6f6;}
	
	.dropdown-list{
		max-width: 335px;
		width: 335px;
	}
	
	.dropdown-list li{ padding: 5px 10px; clear: both;}
	.dropdown-list li:hover{ background: #f2f2f2;}
	.dropdown-list .divider{ margin: 0 1px; padding: 0;}
	
	.dropdown-list a{ display: inline; padding: 0; color: #390 !important;}
	
	.dropdown-list .avatar{
		float: left;
		margin-right: 10px;
	}

	.dropdown-list .info{
		width: 80%;
		float: left;
	}
	.dropdown-list .info span{ font-size: 11px;}
	
	.view-all{
		text-align: center;
		padding: 10px !important;
	}
    
/****************/
/* Thumbnail */
/****************/
	.blog-thumbnail{
		padding: 0;
		border: none;
	}
	.no-shadow{
		-moz-box-shadow: none;
		-webkit-box-shadow: none;
		box-shadow: none;
	}
	.thumbnail .status{
		bottom: 0;
		right: 0;
	}
	.posted{
		font-size: 11px;
		margin: 10px 0 0;
	}
	.tag-cats{
		padding: 2px 3px;
		&:hover{
			text-decoration: none;
			background: #61A534;
			color: #fff;
		}
	}
	.blog-thumbnails li{
		float: none !important;
		min-height: 255px;
		display: -moz-inline-stack;
		display: inline-block;
		vertical-align: top;
		zoom: 1;
		*display: inline;
		_height: 255px;
	}
	.caption {
		a {
			color: #333;
			text-decoration: none;
			&:hover {
				color: #999;
			}
		}
		.posted a {
			color: #61A534;
			&:hover {text-decoration: underline;}
		}
	}
	
/****************/
/* manage story */
/****************/
	.ic-action-story {
		background: url("../img/btn.png") no-repeat;
		display: block;
		height: 16px;
		width: 16px;
		text-indent: -9999px;
		overflow: hidden;
	}
	.ic-edit-story { background-position: 0 -100px; }
	.ic-delete-story { background-position: 0 -150px; }
	


/**************/
/* Filter place */
/**************/
.filter-place {
	float: left;
	margin-left: 30px;
}
	


/*****************/
/* Media Queries */
/*****************/
	@media only screen and (-webkit-min-device-pixel-ratio: 2){
	
	}
	
	@media (min-width: 1200px) { 
	  .grid-colspan{ width: 98.8%;}
	  
	  .blog-thumbnails > li { margin-left: 27px; }
	  
  }
  
  /*----- Large desktop -----*/
	@media (min-width: 1024px) and (max-width: 1200px) { 
		/* Grid */
	  .grid{ width: 180px;}
	  .grid-colspan{ width: 560px;}
	  
	  .blog-thumbnails > li { margin-left: 17px; }
  }
	
	@media (min-width: 981px) and (max-width: 1023px) {
	  /* Grid */
	  .grid{ width: 180px;}
	  .grid-colspan{ width: 560px;}
	  
	  .blog-thumbnails > li { margin-left: 17px; }
  }
	
	/*----- Portrait tablet to landscape and desktop -----*/
  @media (min-width: 768px) and (max-width: 980px) { 
	  /* Menu */
	  .menu{ clear: both; float: none;}
	  .menu li{ margin: 10px 0 20px;}
	  
	  /* Blog */
	  .blog-profile .porfile-photo{ margin-bottom: 5px; }
	  .blog-profile .profile-name{ clear: both; }
	  .blog-profile{ margin-bottom: 20px; }
	  
	  /* Grid */
	  .grid{ width: 172px;}
	  .grid-colspan{ width: 718px;}
	  
	  .blog-thumbnails > li { margin-left: 17px; }
	  
	  .categories-list{
		  float: none;
		  clear: both;
		  width: 100%;
		  margin: 0 0 10px;
	  }
	  
		.filter-title{ float: none; }	  
		.filter-wrap{ margin-top: 20px;}
	  
	  .mood-res{ width: 400px;}
  }
	
	/*----- Landscape phone to portrait tablet -----*/
  @media (max-width: 768px) {
	  /* Menu */
	  .menu{ clear: both; float: none;}
	  .menu li{ margin: 10px 0 20px;}
  
	  .container .span8, .container .span3{

	  }
	  
		.blog-profile{ margin-bottom: 20px; }
		.row{ margin-left: 0; }
		.login{ right: 20px; }
		
		/* Grid */
	  .grid{ width: 166px;}
	  .grid-colspan{ width: 694px;}
		
		.categories-list{
		  float: none;
		  clear: both;
		  width: 100%;
		  margin: 0 0 10px;
	  }
		.categories-list a{
			padding: 10px;
		}
		
		.filter-title{ float: none; }
		.filter-wrap{ margin-top: 20px;}
		
  }
	
	@media (max-width: 730px) {
	  /* Grid */
	  .grid{ width: 160px;}
	  .grid-colspan{ width: 670px;}
  }
  
	/*----- Landscape phones and down -----*/
  @media (max-width: 480px) { 
	  .form-backend { padding-left: 0px; }
	  #header { height: 65px; }
	  #logo { margin: 8px 0 0; }
		span.slogan { font-size: 32px; }

		span.brand {
			font-size: 16px;
			line-height: 22px;
		}
		
		#navigation{ margin: 0;}
		#navigation h1 { margin: 10px 0 0;}
		
		/* Grid */
	  .grid{ width: 138px;}
	  .grid-colspan{ width: 435px; height: 100px;}
	  
	  .grid-colspan .slogan{ font-size: 18px; line-height: 26px;}
	  
	  .grid-colspan .slogan strong{ font-size: 20px;}
		.grid-colspan .title-slogan{ margin-top: 25px;}
		
		#navigation { margin-bottom: 10px;}
		
		.blog-thumbnails [class*="span"] {
			width: 200px;
			min-height: 225px;
		}
		
		.mood-res{ width: 300px;}
		.filter-place { margin-left: 0; }
		.filter-wrap{ margin-top: 20px; }
		
		.menu{
			li{
				border-top: 1px dotted #ccc;
				border-bottom: 1px dotted #ccc;
				padding: 10px 0 10px 5px;
			}
			a{
				display: inline;
				margin: 0 5px 0 0 !important;
				padding-left: 7px;
				width: auto !important;
				background: none;
				font-size: 16px;
				text-align: center;
				text-indent: 0em;
			}
			.popular{ border-left: 3px solid #72b64a;}
			.place{ border-left: 3px solid #f1bd36;}
			.moods{ border-left: 3px solid #573078;}
			.categories{ border-left: 3px solid #d04187;}
			.stories{ border-left: 3px solid #12a3d4;}
		}
		
		
		input[class*="span"], select[class*="span"], textarea[class*="span"], .uneditable-input{
			margin-bottom: 9px
		}
		
  }
  
  @media (max-width: 320px) { 
	  /* Grid */
	  .grid{ width: 133px;}
	  .grid-colspan{ width: 280px; height: 70px;}
	  
	  .grid-colspan .slogan{ font-size: 10px; line-height: 18px;}
	  
	  .grid-colspan .slogan strong{ font-size: 14px;}
		.grid-colspan .title-slogan{ margin-top: 15px;}
		
		#navigation { margin-bottom: 20px;}
		
		.blog-thumbnails [class*="span"] {
			width: auto;
		}
		
		.mood-res{ width: 280px;}
		
		.menu{
			width: 280px;
			ul{ border-top: 1px dotted #ccc;}
			li{
				width: 130px;
				margin: 0;
				border: none;
				border-bottom: 1px dotted #ccc;
			}
		}
  }



/************/
/* Clearfix */
/************/
	.clearfix:after {
		content: ".";
		display: block;
		clear: both;
		visibility: hidden;
		line-height: 0;
		height: 0;
	}
	.clearfix { display: inline-block; }
	html[xmlns] .clearfix { display: block; }
	* html .clearfix { height: 1%; }



/****************/
/* Login page */
/****************/
	.login_form {
		min-height: 500px; /* min-height hack for ie6 */
		height: auto !important;
		height: 500px;
		margin-top: 40px;
		.oxfam-font {margin-bottom: 20px;}
		form { padding: 25px 30px; }
	}



/****************/
/* Upload Image */
/****************/
  .image-help{
    position: absolute;
    top: 10px;
    left: 80px;
    width: 225px;
    height: 40px;
    text-indent: -999em;
    background: url('../img/ic-add-photo.png') no-repeat ;
  }
  
  .input-file{
    position: relative;
    border: none;
    -moz-opacity: 0;
    opacity: 0;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=0);
    background: transparent !important;
    z-index: 999;
  }
    
  .drop-area{ position: relative; }
  .dropbox{
		width: 102px;
		height: 102px;
		background: url('../img/bg-drop.png') no-repeat !important;
		.image-append{
			width: 102px;
			height: 102px;
		}
	}
	
	.select-file{
		position: absolute;
		bottom: 10px;
		left: 170px;
		width: 104px;
		height: 28px;
		background: url('../img/btn-select-file.png') no-repeat !important;
		&:hover{ cursor: pointer; }
	}

	.ic{
		display: block;
		margin: 10px 0;
		padding-left: 16px;
		background: url('../img/ic-moods.png') no-repeat;
	}
	.image-delete{ background-position: -90px 0; }


	/* foo */
	.image-border {
    background: transparent ;
    background: #f0f0f0;
    border: 1px dashed #c4c4c4;
    width: 280px;
    height: 150px;
  }
		
    input.fileupload-classic {
        width: 100%;
    }
    
    .image-upload {
        position: relative;
        height: 140px;
    }
    
    form#blog-form .image-upload {
        position: relative;
        height: auto;
    }
    
    #fileupload-progress-wrapper {
        position: absolute;
        margin-top: 10px;
        margin-bottom: 70px;
        display: none;
    }
    #fileupload-progress {
        width: 200px;
        display: none;
    }

    form#blog-form .dropbox {
        background: #f0f0f0 url('../img/add_photo.png') center center no-repeat !important;
        width: 280px;
        height: 150px;
    }

    form#blog-form .dropbox .image-append {
        height: 150px;
    }
    
    form#blog-form #fileupload-progress-wrapper {
        margin: 55px 0 0 40px;
    }
    
    
/*  tag blog    */

    div.controls ul.tagit { 
        margin: 0;
        width: 300px;
        font-size: 12px;
    }
    ul.tagit input[type="text"] {
        -moz-box-shadow: none; 
        -webkit-box-shadow: none;
        box-shadow: none;
    }





/*****************/
/* Scour         */
/*****************/
.scour {
    position: relative;
    margin: 0 auto;
}
.scour .rect {
    position: absolute;
    background-color: #DDD;
}
.scour .has_blog {
    background-color: #FFF;
}

.black-layer {
    width: 100%;
    height: 100%;
    background-color: #FFF;
    position: absolute;
    top: 0px;
    left: 0px;
    display: none;
}

.popover-inner {
    padding: 3px;
    width: 280px;
    overflow: hidden;
    /*background: url(../img/bg-black80.png);*/
    -webkit-box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
    box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
}
.popover-title {
    padding: 9px 15px 0;
    line-height: 1.5em;
    background: none;
    border-bottom: 0px;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    color: #fff;
    font-size: 1.1em;
}
.popover-content {
    padding: 14px;
    background: none;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    color: #a7a7a7;
    font-size: 1em;
}

/* view blog : tag */
.blog-tag h3, .download-photo h3 {
	border-bottom: 1px solid #ddd;
	font-size: 16px;
	margin-bottom: 10px;
	padding-bottom: 5px;
}

.blog-tag {
	overflow: hidden;
	width: 370px;
	.badge {
		color: #fff !important;
		margin: 0 5px 5px 0;
		float: left;
		&:hover {
			background-color: #666;
		}
	}
}

/* view blog : photo */
.download-photo {
	ul {
		margin-left: 0;
	}
	li {
		list-style-type: none;
		border-bottom: 1px solid #ddd;
		padding: 10px 0;
		overflow: hidden;
		width: 370px;
	}
	.ic-size {
		background-position: 0 -50px !important;
	}
	.ic-download {
		background-position: 4px 3px !important;
	}
	.col-left {
		padding-left: 25px;
		background: url("../img/ic-blog.png") no-repeat;
	}
}

.col-left, .col-right{ float: left; vertical-align: top;}
.col-left{ width: 25%;}
.col-right{ width: 65%;}


/*****************/
/* Crosalot zone */
/*****************/
.messages .tags {
    font-weight: bold;
      float: left;
      line-height: 15px;
      padding-right: 5px;
}
.filter-block-append {
    overflow: hidden;
}
.filter-block-append .default {
    z-index: 5;
}
.filter-menu {
    background-color: #FFF;
    z-index: 10;
    width: 100%;
