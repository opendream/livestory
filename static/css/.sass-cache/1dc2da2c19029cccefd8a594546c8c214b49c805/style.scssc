	.login_form {
		min-height: 500px; /* min-height hack for ie6 */
		height: auto !important;
		height: 500px;
		margin-top: 40px;
		.oxfam-font {margin-bottom: 20px;}
		form { padding: 25px 30px; }
	}




    
    






/*****************/
/* Scour         */
/*****************/
.scour {
    position: relative;
    margin: 0 auto;
}
.scour .rect {
    position: absolute;
    background-color: #DDD;
}
.scour .has_blog {
    background-color: #FFF;
}

.black-layer {
    width: 100%;
    height: 100%;
    background-color: #FFF;
    position: absolute;
    top: 0px;
    left: 0px;
    display: none;
}

.popover-inner {
    padding: 3px;
    width: 280px;
    overflow: hidden;
    /*background: url(../img/bg-black80.png);*/
    -webkit-box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
    box-shadow: 0 3px 7px rgba(0, 0, 0, 0.3);
}
.popover-title {
    padding: 9px 15px 0;
    line-height: 1.5em;
    background: none;
    border-bottom: 0px;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    color: #fff;
    font-size: 1.1em;
}
.popover-content {
    padding: 14px;
    background: none;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
    color: #a7a7a7;
    font-size: 1em;
}

/* view blog : tag */
