/**************/
/* Base Style */
/**************/
	@font-face { 	/*----- Oxfam's font-face -----*/
		font-family: 'OxfamGlobalHeadlineRegular';
		src: url('../fonts/oxfamglobalheadline-webfont.eot');
		src: url('../fonts/oxfamglobalheadline-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/oxfamglobalheadline-webfont.woff') format('woff'),
		url('../fonts/oxfamglobalheadline-webfont.ttf') format('truetype'),
		url('../fonts/oxfamglobalheadline-webfont.svg#OxfamGlobalHeadlineRegular') format('svg');
		font-weight: normal;
		font-style: normal;
	}
	
	@font-face {
		font-family: 'FuturaMdCnBTMedium';
		src: url('../fonts/futuramc-webfont.eot');
		src: url('../fonts/futuramc-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/futuramc-webfont.woff') format('woff'),
		url('../fonts/futuramc-webfont.ttf') format('truetype'),
		url('../fonts/futuramc-webfont.svg#FuturaMdCnBTMedium') format('svg');
		font-weight: normal;
		font-style: normal;
	}
    
	.oxfam-font{ font-family: OxfamGlobalHeadlineRegular; }
	.futura-font { font-family: FuturaMdCnBTMedium; }
	
	/*----- Title bullet icon -----*/
	.ic-page-title{
		padding: 0 0 0 25px;
		background: url('../img/ic-title.png') 0px 10px no-repeat ;
	}
	.ic-title-pink { background: url('../img/ic-title_pink.png') 0px 10px no-repeat; }
	.ic-title-purple { background: url('../img/ic-title_purple.png') 0px 10px no-repeat; }
	.ic-title-blue { background: url('../img/ic-title_blue.png') 0px 10px no-repeat; }
	.ic-title-yellow { background: url('../img/ic-title_yellow.png') 0px 10px no-repeat; }
	.ic-title-grey { background: url('../img/ic-title_grey.png') 0px 10px no-repeat; }
    
  .content{
		border: 1px solid #eee;
		padding: 8px 35px 8px 20px;
		margin-bottom: 18px;
		background: #fff;
  }
  
  @mixin border-rounded($r){
		-webkit-border-radius: $r;
		-moz-border-radius: $r;
		border-radius: $r;
	}
	.rounded{ @include border-rounded(4px); }
	.rounded5{ @include border-rounded(5px); }
	.rounded10{ @include border-rounded(10px); }
	
  .block{
		margin-bottom: 24px !important;
  }
  .label-inline-form{
		line-height: 25px;
  }
  .separate{
		background: #5e5e5e;
		width: 1px;
		height: 20px;
		margin: 0 10px 0 10px;
	}
  .tab-menu{ margin: 20px 0;}
	.clear-both{ clear: both;}
	.img-vertical{ vertical-align: middle; margin: 0 5px 0 0;}
	.hide{ display: none !important;}
	.active-b{ font-weight: bold; color: #000 !important;}
	.mood-res{ width: 550px;}
	.typeahead a{ background: #fff !important;
		&:hover{ background: #F2F2F2 !important; text-decoration: underline !important;}
	}
	
	/*----- Form -----*/
	legend, .page-title{ padding: 10px 7px 10px; border-bottom: 1px solid #EEE;}
	legend h1, h1{ font: normal 19.5px OxfamGlobalHeadlineRegular; }
	.form-backend { padding-left: 160px; }
	
	/*----- Link -----*/
	a:link { color:#390; }
	a:visited { color:#339900; }
	a:hover { text-decoration: underline;} 
	a:active { color:#000; }
	
	::-webkit-input-placeholder {color: #c6c6c6; }
	:-moz-placeholder { color: #c6c6c6; }
  
	/*----- Color -----*/
	.grey{ color: #999; }
	
	/*----- Base Color Menu -----*/
	.green{ color: #72b64a;}
	.yellow{ color: #f1bd36;}
	.purple{ color: #573078;}
	.pink{ color: #d04187;}
	.blue{ color: #12a3d4;}
  
  /*----- Button -----*/
  @mixin btn{
	  @include border-rounded(6px);
	  display: inline-block;
    color: white;
    font: bold 13px arial;
    padding: 7px 12px;
    margin-right: 10px;
    text-decoration: none;
    *padding: 3px 12px;
  }
  .btn-green{
	  @include btn;
    border: 1px solid #089D4B;
    -moz-box-shadow: inset -1px -2px 0px 0px #3f9303;
    -webkit-box-shadow: inset -1px -2px 0px 0px #3f9303;
    box-shadow: inset -1px -2px 0px 0px #3f9303;
    background-color: #72B64A;
    &:hover{
	    border: 1px solid #0A5A26;
	    -moz-box-shadow: inset -1px -2px 0px 0px #0a5a26;
	    -webkit-box-shadow: inset -1px -2px 0px 0px #0a5a26;
	    box-shadow: inset -1px -2px 0px 0px #0a5a26;
	    background-color: #0F7335;
    }
  }
  
  .btn-grey {
	  @include btn;
    border: 1px solid #787878;
    -moz-box-shadow: inset -1px -2px 0px 0px #636363;
    -webkit-box-shadow: inset -1px -2px 0px 0px #636363;
    box-shadow: inset -1px -2px 0px 0px #636363;
    background-color: #A0A0A0;
    &:hover{
	    border: 1px solid #696969;
	    -moz-box-shadow: inset -1px -2px 0px 0px #535353;
	    -webkit-box-shadow: inset -1px -2px 0px 0px #535353;
	    box-shadow: inset -1px -2px 0px 0px #535353;
	    background-color: #787878;
    }
  }

  /*----- Grid -----*/
  .grid-row{ clear: both;  margin-left: -10px;}
	.grid{
		float: left;
		overflow: hidden;
		width: 184px;
		margin: 0 0 10px 10px;
	}
	.grid-colspan{
		width: 573px;
		height: 135px;
		background: #f2f2f2;
	}
	.grid-colspan .slogan{
		font: lighter 24px/36px "Helvetica Neue";
		clear: both;
		margin-right: 20px;
		text-align: right;
	}
	.grid-colspan .title-slogan{ margin-top: 30px;}
  
  /* fufu */
  .image-append img {
    border: 1px dashed #BBB;
    padding: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
  }
  
  
    
/***********/
/* Top bar */
/***********/
	#topbar{
		height: 26px;
		padding: 8px 0 6px;
		background: #000;
		.home{
			float: left;
			margin: 3px 0 0;
			padding: 0 0 0 20px;
			line-height: 22px;
			background: url('../img/topbar-widget.png') no-repeat 0 1px;
			text-decoration: none;
		}
	}
	
	.topbar-widget a, #topbar .home{ color: #999;}
	.topbar-widget a:hover, #topbar .home:hover{ color: #fff; text-decoration: none;}
	.topbar-widget{ float: right; }
	
	.notify, .post-blog, .invite {margin-top: 2px;}
	.post-blog{
		width: 17px; height: 17px;
		a{
			display: block;
			width: 16px;
			height: 16px;
			text-indent: -999em;
			background: url('../img/topbar-widget.png') no-repeat 0 -39px;
			&:hover { background-position: -16px -39px; }
		}
	}
	.invite a{
		padding: 0 0 0 15px;
		text-decoration: none;
		background: url('../img/topbar-widget.png') no-repeat 0 -61px;
	}

	/*------ Profile dropdown ------*/
	.profile-toggle{
		display: block;
		width: 15px;
		height: 23px;
	}
	.profile-toggle .caret{
		margin-left: 3px;
		border-left: 4px solid transparent;
		border-right: 4px solid transparent;
		border-top: 4px solid #fff;
	}
	.profile-toggle img{ vertical-align: middle; margin-right: 5px;}
	
	/*----- Dropdown -----*/
	.caret{ opacity: 0.5;}
	.dropdown .dropdown-menu{
		top: 32px;
		left: 9px;
	}
	.dropdown-menu li a {
		color:#390 !important;
		&:hover { background: #f2f2f2; text-decoration: underline; }
	}
	.notify{
		padding: 2px 7px 2px 6px; 
		margin-top: 0;
		font-size: 11px;
		color: #fff !important;
		background: #ea1515;
	}
	.no-notify{
		color: #afafaf !important;
		background: #585858 !important;
	}
	.notify-bottom{
		display: block;
		width: 7px;
		height: 5px;
		margin: 0 0 0 8px;
		background: url('../img/topbar-widget.png') no-repeat 0 -25px;
	}


/******************/
/* Header Section */
/******************/
	#header{
		height: 90px;
		background: url('../img/bg_header.png') repeat-x 0px -20px ;
	}
	#header .container{ position: relative; }
	#logo{
