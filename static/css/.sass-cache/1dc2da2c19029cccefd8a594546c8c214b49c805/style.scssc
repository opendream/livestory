	  #logo { margin: 8px 0 0; }
		span.slogan { font-size: 32px; }

		span.brand {
			font-size: 16px;
			line-height: 22px;
		}
		
		#navigation{ margin: 0;}
		#navigation h1 { margin: 10px 0 0;}
		
		/* Grid */
	  .grid{ width: 135px;}
	  .grid-colspan{ width: 425px; height: 100px;}
	  
	  .grid-colspan .slogan{ font-size: 18px; line-height: 26px;}
	  
	  .grid-colspan .slogan strong{ font-size: 20px;}
		.grid-colspan .title-slogan{ margin-top: 25px;}
		
		#navigation { margin-bottom: 10px;}
		
		.blog-thumbnails [class*="span"] {
			width: 200px;
			min-height: 225px;
		}
		
		.mood-res{ width: 300px;}
		.filter-place { margin-left: 0; }
		.filter-wrap{ margin-top: 20px; }
		
		.menu{
			li{
				border-top: 1px dotted #ccc;
				border-bottom: 1px dotted #ccc;
				padding: 10px 0 10px 5px;
			}
			a{
				display: inline;
				margin: 0 5px 0 0 !important;
				padding-left: 7px;
				width: auto !important;
				background: none;
				font-size: 16px;
				text-align: center;
