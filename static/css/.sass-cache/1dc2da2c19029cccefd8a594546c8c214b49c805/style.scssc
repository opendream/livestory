	  .menu li{ margin: 10px 0 20px;}
  
	  #footer{
			.span7{ text-align: left; }
		}
	  
		.blog-profile{ margin-bottom: 20px; }
		.row{ margin-left: 0; }
		.login{ right: 20px; }
		
