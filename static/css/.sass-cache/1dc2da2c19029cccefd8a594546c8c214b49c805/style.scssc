/****************/
/* Filter */
/****************/
	.filter-block { position: relative; }
	.filter-wrap{ margin-bottom: 10px;}
	.filter-title {
		float: left;
		padding: 15px 15px 10px;
		font-size: 24px;
		font-weight: normal;  
		background-color: #f7f7f7;
		text-transform: uppercase;
		-moz-box-shadow: inset 0 0 10px #dcdcdc;
		-webkit-box-shadow: inset 0 0 10px #dcdcdc;
		box-shadow: inset 0 0 10px #dcdcdc;
	}
	
	.btn-hide {
		position: absolute;
		display: block;
		overflow: hidden;
		bottom: 0;
		right: 0;
		height: 22px;
		width: 59px;
		text-indent: -999px;
		background: url("../img/btn.png") no-repeat;
		&:hover { background-position: 0 -22px; }
	}
	
	.filter-bottom, .filter-top {
		display: block;
		height: 20px;
		clear: both;
		background: url("../img/bg-filter_mood_bottom.png") no-repeat center bottom;
	}
	.filter-top { background: url("../img/bg-filter_mood_top.png") no-repeat center top; }



/**************/
/* Categories */
/**************/
	.categories-list {
		float: right;
		width: 68%;
	}
	.list-block {
	  border-top: 1px solid #dadada;
	  border-bottom: 1px solid #dadada;
	  border-right: 1px solid #dadada;
	  border-left: 3px solid #D04187;
	  list-style: none;
	  float: left;
	  text-align: left;
	  margin: 0 20px 5px 0;
	  a{
	    color: #000;
	    padding: 3px 5px;
	    display: block;
	    text-decoration: none;
	    &:hover{ background: #eaeaea; }
	  }
	  a.active { background: #eaeaea; font-weight: bold;}
	}


/*******/
/* Tag */
/*******/
	div.controls ul.tagit { 
	    margin: 0;
	    width: 300px;
	    font-size: 12px;
	}
	ul.tagit input[type="text"] {
	    -moz-box-shadow: none; 
	    -webkit-box-shadow: none;
	    box-shadow: none;
	}

    
    
/****************/
/* Notification */
/****************/
	.ic-status{
		float: left;
		width: 12px;
		height: 12px;
		margin: 3px 5px 0 0;
		text-indent: -999em;
		background: url('../img/ic-notify.png') no-repeat ;
	}
	.ic-download-notify{ background-position: 0 -12px;}
	
	.avatar{ width: 25px;}
	.ic-notify{
		width: 12px;
		padding: 0 !important;
	}
	.middle{ line-height: 19px !important; /* vertical-align: middle !important; */}
	.middle .grey{ font-size: 11px; }
	.middle .ic-status {margin: 13px 0 0 0;}
	
	.table-list{ 
		margin-top: 20px;
		border-top: 1px solid #acacac;
	}
	.table-list th, .table-list td{ border-bottom: 1px solid #ddd; }
	.table-list th {
		background-color: #d7d7d7;
		border-bottom: 1px solid #acacac;
		
		a{ color: #363636 !important; }
	}
	.table-list .centered{ text-align: center;}
	.table-striped tbody tr:nth-child(odd) td,
	.table-striped tbody tr:nth-child(odd) th{ background: #f6f6f6;}
	.sort-asc, .sort-desc{
		display: inline-block;	
		width: 10px;
		height: 6px;
		text-indent: -999em;
		opacity: 0.65;
		background: url('../img/btn.png') 0px -200px no-repeat;
	}
	.sort-desc{ background-position: 0 -250px; }
	
	.dropdown-list{
		max-width: 300px;
		width: 300px;
	}
	
	.dropdown-list li{ padding: 5px 10px; clear: both;}
	.dropdown-list li:hover{ background: #f2f2f2;}
	.dropdown-list .divider{ margin: 0 1px; padding: 0;}
	
	.dropdown-list a{ display: inline; padding: 0; color: #390 !important;}
	
	.dropdown-list .avatar{
		float: left;
		margin-right: 10px;
	}

	.dropdown-list .info{
		width: 80%;
		float: left;
	}
	.dropdown-list .info span{ font-size: 11px;}
	
	.view-all{
		text-align: center;
		padding: 10px !important;
	}
    
/****************/
/* Thumbnail */
/****************/
	.blog-thumbnail{
		padding: 0;
		border: none;
	}
	.no-shadow{
		-moz-box-shadow: none;
		-webkit-box-shadow: none;
		box-shadow: none;
	}
	.thumbnail .status{
		bottom: 0;
		right: 0;
	}
	.posted{
		font-size: 11px;
		margin: 10px 0 0;
	}
	.tag-cats{
		padding: 2px 3px;
		&:hover{
			text-decoration: none;
			background: #61A534;
			color: #fff;
		}
	}
	.blog-thumbnails li{
		float: none !important;
		min-height: 255px;
		display: -moz-inline-stack;
		display: inline-block;
		vertical-align: top;
		zoom: 1;
		*display: inline;
		_height: 255px;
	}
	.caption {
		a {
			color: #333;
			text-decoration: none;
			&:hover {
				color: #999;
			}
		}
		.posted a {
			color: #61A534;
			&:hover {text-decoration: underline;}
		}
	}
	
/****************/
/* manage story */
/****************/
	.ic-action-story {
		background: url("../img/btn.png") no-repeat;
		display: block;
		height: 16px;
		width: 16px;
		margin: 0 auto;
		text-indent: -9999px;
		overflow: hidden;
	}
	.ic-edit-story { background-position: 0 -100px; }
	.ic-delete-story { background-position: 0 -150px; }
	


/**************/
/* Filter place */
/**************/
.filter-place {
	float: left;
	margin-left: 30px;
}



/****************/
