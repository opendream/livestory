/***********/
/* Top bar */
/***********/
	#topbar{
		height: 26px;
		padding: 8px 0 6px;
		background: #000;
		.home{
			float: left;
			margin: 3px 0 0;
			padding: 0 0 0 20px;
			line-height: 22px;
			background: url('../img/topbar-widget.png') no-repeat 0 1px;
			text-decoration: none;
		}
	}
	
	.topbar-widget a, #topbar .home{ color: #999;}
	.topbar-widget a:hover, #topbar .home:hover{ color: #fff; text-decoration: none;}
	.topbar-widget{ float: right; }
	
	.notify, .post-blog, .invite {margin-top: 2px;}
	.post-blog{
		width: 17px; height: 17px;
		a{
			display: block;
			width: 16px;
			height: 16px;
			text-indent: -999em;
			background: url('../img/topbar-widget.png') no-repeat 0 -39px;
			&:hover { background-position: -16px -39px; }
		}
	}
	.invite a{
		padding: 0 0 0 15px;
		text-decoration: none;
		background: url('../img/topbar-widget.png') no-repeat 0 -61px;
	}

	/*------ Profile dropdown ------*/
	.profile-toggle{
		display: block;
		width: 15px;
		height: 23px;
	}
	.profile-toggle .caret{
		margin-left: 3px;
		border-left: 4px solid transparent;
		border-right: 4px solid transparent;
		border-top: 4px solid #fff;
	}
	.profile-toggle img{ vertical-align: middle; margin-right: 5px;}
	
	/*----- Dropdown -----*/
	.caret{ opacity: 0.5;}
	.dropdown .dropdown-menu{
		top: 32px;
		left: 9px;
	}
	.dropdown-menu li a {
		color:#390 !important;
		&:hover { background: #f2f2f2; text-decoration: underline; }
	}
	.notify{
		padding: 2px 7px 2px 6px;
		margin-top: 0;
		font-size: 11px;
		color: #fff !important;
		background: #ea1515;
	}
	.no-notify{
		color: #afafaf !important;
		background: #585858 ;
	}
	.notify-bottom{
		display: block;
		width: 7px;
		height: 5px;
		margin: 0 0 0 8px;
		background: url('../img/topbar-widget.png') no-repeat 0 -25px;
	}


/******************/
/* Header Section */
/******************/
	#header{
		height: 90px;
		background: url('../img/bg_header.png') repeat-x 0px -20px ;
	}
	#header .container{ position: relative; }
	#logo{
		float: left;
		width: 128px;
		height: 48px;
		display: block;
		margin: 20px 0 0;
		text-indent: -999em;
		background: url('../img/logo-oxfam.png') no-repeat ;
	}
	.login{
		position: absolute;
		top: 0;
		right: 0;
		width: 80px;
		padding: 15px 0;
		text-align: center;
		background: #000;
		a{ 
			font-size: 12px; color: #fff; 
			&:hover{ text-decoration: none; border-bottom: 1px dotted #fff; }
		}
	}
        
	/*----- menu -----*/
	.menu{
		float: right;
		width: 470px;
		list-style: none;
		margin: 4px 0 0;
		li { float: left;}
		a{
			display: block;
			height: 69px;
			display: block;
			text-indent: -999em;
			background: url('../img/nav.png') no-repeat;
		}
		.popular{
			width: 65px;
			margin-right: 25px;
		}
		.place{
			width: 62px;
			margin-right: 28px;
			background-position: -65px 0;
		}
		.moods{
			width: 62px;
			margin-right: 25px;
			background-position: -127px 0;
		}
		.categories{
			width: 89px;
			margin-right: 25px;
			background-position: -189px 0;
		}
		.stories{
			width: 86px;
			background-position: -278px 0;
		}
		
	}

	/*----- Active & Hover -----*/
	.popular:hover, .popular.active{ background-position: 0 -69px; }
	.place:hover, .place.active{ background-position: -65px -69px; }
	.moods:hover, .moods.active{ background-position: -127px -69px; }
	.categories:hover, .categories.active{ background-position: -189px -69px; }
	.stories:hover, .stories.active{ background-position: -278px -69px; }

	/*----- Admin Menu Sidebar -----*/
	.nav-admin .active > a, .nav-admin .active > a:hover{ background: #72b64a; }
	
	.nav-admin .nav-header a{ color: #999; }
	.nav-admin .nav-header a span{
		float: right;
		padding: 0px 7px;
		color: #fff;
		font-weight: normal;
		font-size: 11px;
		background: #EA1515;
	}
	.nav-admin > li + .nav-header { margin-top: 0; }
	.nav-admin .nav-header{ border-top: 1px dotted #AAA; }
	.nav-admin > li:hover { background-color: #ebebeb; }
        
	/*----- Search -----*/
	.search-stories{
		float: left;
		width: 182px;
		height: 12px;
		padding: 8px 12px;
		margin: 0 1px 0 0;
		border-top: 3px solid #ececec;
		border-left: 2px solid #ececec;
		background: #fff;
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
	}
	
	.btn-search{
		width: 47px;
		height: 32px;
		border: 0;
		border-top: 3px solid #ececec;
		border-left: 1px solid #fff;
		border-right: 1px solid #fff;
		border-bottom: 1px solid #fff;
		text-indent: -999em;
		background: #fff url('../img/btn-search.png') no-repeat 10px 2px;
		-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
		-moz-box-sizing: border-box;    /* Firefox, other Gecko */
		box-sizing: border-box;         /* Opera/IE 8+ */
	}

	/*----- Brand -----*/
	span.brand{
		display: block;
		font-size: 18px;
		font-weight: normal;
		color: #72b64a;
	}
	#navigation h1{ float: left;}
	span.slogan{ font-size: 44px; }


/********/
/* Blog */
/********/
	.blog-container-photo{
		float: left;
		margin: 0 auto 18px;
	}
	.blog-photo{
		position: relative;
		padding: 10px;
		border: 1px solid #d6d6d6;
		-moz-box-shadow: 3px 3px 0px #e3e3e3;
		-webkit-box-shadow: 3px 3px 0px #e3e3e3;
		box-shadow: 3px 3px 0px #e3e3e3;
	}
	.status{
		position: absolute;
		bottom: 10px;
		right: 10px;
		padding: 3px 5px 3px 3px;;
		color: #ddd;
		font-size: 11px;
		line-height: 16px;
		background: rgba(0,0,0,0.5);
	}
	#title-blog{
		clear: both;
		line-height: 28px;
		margin: 0 0 10px;
		font-size: 18px;
	}
	
	.blog-profile{ 
		margin-bottom: 40px;
		.porfile-photo{
			float: left;
			width: 94px;
			height: 94px;
			margin: 0 10px 5px 0;
			border: 3px solid #000;
		}
		.profile-name{
			display: block;
			margin-bottom: 5px;
			font-size: 18px;
			font-weight: bold;
			color: #000;
		}
	}
    
	/*----- Love it -----*/
	.love, .unlove{
		display: block;
		float: left;
		width: 102px;
		height: 33px;
		margin: 0 10px 0 0;
		text-indent: -999em;
		background: url('../img/btn-loveit.png') no-repeat ;
	}
	.unlove{ background-position: 0 -66px;
		&:hover{ background-position: 0 -98px;}
	}
	.love:hover{ background-position: 0 -33px; }
	
	.people-love{
		padding: 0 0 0 20px;
		font-size: 12px;
		line-height: 28px;
		background: url('../img/ic-loveit.png') no-repeat ;
	}
	.love-sum{
		font-weight: bold;
		font-size: 18px;
		color: #333;
	}
    
	/*----- Friends -----*/
	.friends{
		margin: 20px 0 0;
		li{
			position: relative;
			float: left;
			width: 35px;
			height: 35px;
			margin: 0 7px 7px 0;
			list-style: none;
		}
	}
	
	/*----- Pop up -----*/
	.friends-inline{
		margin: 0;
		overflow: hidden;
		width: 100%;
		li{
			float: left;
			width: 46%;
			margin: 1% 2% 1% 0;
			padding: 5px;
			list-style: none;
			&:hover{ background: #eee; }
		}
		img{
			float: left;
			margin: 0 10px 0 0;
		}
	}
    
	/*----- Tooltip -----*/
	.tooltip-popup{
		display: none;
		height: 15px;
		padding: 7px 10px;
		white-space: nowrap;
		text-align: center;
		color: #fff;
		background: #333;
		z-index: 9999;
	}
	.friends li:hover .tooltip-popup{
		display: block;
		position: absolute;
		left: 0;
		top: 50px;
		
		/*----- CSS Animation -----*/
		-moz-animation-name: pop;
	  -moz-animation-duration: 300ms;
	  -moz-animation-fill-mode: forwards;
	  -webkit-animation-name: pop;
	  -webkit-animation-duration: 300ms;
	  -webkit-animation-fill-mode: forwards;
	  animation-name: pop;
	  animation-duration: 300ms;
	  animation-fill-mode: forwards;
	}
	.tooltip-popup:after {
	  position: absolute;
	  left: 15px;
	  bottom: 29px;
	  width: 0;
	  content: "";
	  display: block;
	  border-color: #333 transparent;
	  border-style: solid;
	  border-width: 0 5px 5px;
	}
	@-webkit-keyframes pop{
		0%{ opacity: 0; top: 45px; }
		100%{ opacity: 1; top: 50px; }
	}
	@-moz-keyframes pop {
	  0%{ opacity: 0; top: 45px; }
		100%{ opacity: 1; top: 50px; }
	}
	@keyframes pop {
	  0%{ opacity: 0; top: 45px; }
		100%{ opacity: 1; top: 50px; }
	}
		
	.more-friend{
		float: right;
		clear: both;
		margin: 10px 0 0;
	}


