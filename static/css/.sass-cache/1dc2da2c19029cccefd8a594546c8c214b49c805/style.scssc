	.login{
		position: absolute;
		top: 0;
		right: 0;
		width: 80px;
		padding: 15px 0;
		text-align: center;
		background: #000;
		a{ 
			font-size: 12px; color: #fff; 
			&:hover{ text-decoration: none; border-bottom: 1px dotted #fff; }
		}
	}
        
	/*----- menu -----*/
	.menu{
		float: right;
		width: 470px;
		list-style: none;
		margin: 4px 0 0;
		li { float: left;}
		a{
			display: block;
			height: 69px;
			display: block;
			text-indent: -999em;
			background: url('../img/nav.png') no-repeat;
		}
		.popular{
			width: 65px;
			margin-right: 25px;
		}
		.place{
			width: 62px;
			margin-right: 28px;
			background-position: -65px 0;
		}
		.moods{
			width: 62px;
			margin-right: 25px;
			background-position: -127px 0;
		}
		.categories{
			width: 89px;
			margin-right: 25px;
			background-position: -189px 0;
		}
		.stories{
			width: 86px;
			background-position: -278px 0;
		}
		
	}

	/*----- Active & Hover -----*/
	.popular:hover, .popular.active{ background-position: 0 -69px; }
	.place:hover, .place.active{ background-position: -65px -69px; }
	.moods:hover, .moods.active{ background-position: -127px -69px; }
	.categories:hover, .categories.active{ background-position: -189px -69px; }
	.stories:hover, .stories.active{ background-position: -278px -69px; }

	/*----- Admin Menu Sidebar -----*/
	.nav-admin .active > a, .nav-admin .active > a:hover{ background: #72b64a; }
	
	.nav-admin .nav-header a{ color: #999; padding-top: 6px; padding-bottom: 6px;}
	.nav-admin .nav-header.active a{ color: #fff !important;}
	.nav-admin .nav-header a span{
		float: right;
		padding: 0px 7px;
		color: #fff;
		font-weight: normal;
		font-size: 11px;
		background: #EA1515;
	}
	.nav-admin > li + .nav-header { margin-top: 0; }
	.nav-admin .nav-header{
		border-top: 1px dotted #AAA; 
		padding: 0 15px;
	}
	.nav-admin > li:hover { background-color: #ebebeb; }
	.nav-admin .notify{ padding: 0px 7px 0px 6px; background: #777;}
	*+html .nav-admin .notify{ margin-top: -21px; margin-right: 3px;} /* IE7 */
        
	/*----- Search -----*/
	.search-stories{
		float: left;
		width: 182px;
		height: 12px;
		padding: 8px 12px;
		margin: 0 1px 0 0;
		border-top: 3px solid #ececec;
		border-left: 2px solid #ececec;
		background: #fff;
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
	}
	
	.btn-search{
		width: 47px;
		height: 32px;
		border: 0;
		border-top: 3px solid #ececec;
		border-left: 1px solid #fff;
		border-right: 1px solid #fff;
		border-bottom: 1px solid #fff;
		text-indent: -999em;
		background: #fff url('../img/btn-search.png') no-repeat 10px 2px;
		-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
		-moz-box-sizing: border-box;    /* Firefox, other Gecko */
		box-sizing: border-box;         /* Opera/IE 8+ */
	}

	/*----- Brand -----*/
	span.brand{
		display: block;
		font-size: 18px;
		font-weight: normal;
		color: #72b64a;
	}
	#navigation h1{ float: left;}
	span.slogan{ font-size: 44px; }


/********/
/* Blog */
/********/
	.blog-container-photo{
		float: left;
		margin: 0 auto 18px;
	}
	.blog-photo{
		position: relative;
		overflow: hidden;
		padding: 10px;
		border: 1px solid #d6d6d6;
		-moz-box-shadow: 3px 3px 0px #e3e3e3;
		-webkit-box-shadow: 3px 3px 0px #e3e3e3;
		box-shadow: 3px 3px 0px #e3e3e3;
		
		img{ float: left; }
	}
	.status{
		position: absolute;
		bottom: 10px;
		right: 10px;
		overflow: hidden;
		padding: 3px 5px 3px 3px;;
